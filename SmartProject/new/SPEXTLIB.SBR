     bedingung$ = "art.master=5"
     art% = 5
   CASE 7
     REM "Gutschrift als Vorlage"
     bedingung$ = "art.master=7"
     art% = 7
   END SELECT 
   IF opt% > 1 AND opt% < 8 THEN 
     IF opt% <> 7 THEN 
       vMSArt% = opt% - 1
     ELSE 
       vMSArt% = opt%
     END IF 
     scode% = knr% * 10 + vMSArt%
