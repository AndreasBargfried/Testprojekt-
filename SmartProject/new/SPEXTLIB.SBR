       END IF 
     NEXT i%
     FOR i% = 0 TO drulen%
       cums% = druckvektor%(i%)
       IF cums% > 0 THEN 
         CALL MSBuchung(cums%,1)
       ELSE 
         i% = drulen%
       END IF 
     NEXT i%
 END SUB 
 
 
 
