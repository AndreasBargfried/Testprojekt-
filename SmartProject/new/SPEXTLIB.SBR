     f.loeschen.enabled = - 1
     f.kopieren.enabled = - 1
     f.eks.enabled = - 1
     f.umsatz.enabled = - 1
     IF abbruch% = - 1 THEN 
       SELECT KEY codegr.gruppen FILE "gruppen" INDEX codegr.gruppen
       FORM 
       abbruch% = 0
     END IF 
   END SELECT 
 WEND 
 _formend% = 0
 cugrp% = 0
 END SUB 
 
 
