 SELECT CASE opt%
 CASE 1
   feld$ = "Titel.FILME"
 CASE 2
   feld$ = "Nummer.FILME"
 CASE 3
   feld$ = "Hauptdarsteller.FILME"
 CASE 4
   feld$ = "Regisseur.FILME"
 CASE 5
   feld$ = "Buch.FILME"
