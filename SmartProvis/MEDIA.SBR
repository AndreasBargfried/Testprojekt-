 ' =======================================================================
 ' Routinen Formular "MEDIA" - HauptmenÅ                         
 ' =======================================================================
 
SUB MEDIA()
 SET HEADING title$ + " - HauptÅbersicht"
 SET STATUS "Hauptauswahl: WÑhlen Sie die gewÅnschte Funktion!"
 _formend% = 0
 CALL Gauge(0,"")
 WHILE _formend% = 0
 MEDIAsub% = 0
   IF FORM <> "MEDIA" THEN 
     CALL MNMEDIA()
     OPEN FORM "MEDIA"
     SET DISPLAY ON 
     SET STATUS "Medienverwaltung"
     CALL HideStartup()
     MENU ON 
   END IF 
   FORM 
   WAIT MOUSE OR KEY OR MENU 
   SELECT CASE MEDIAsub%
   CASE 1
     CALL ADRESSEN()
   CASE 2
     CALL FILME()
   CASE 3
     CALL VLRÅcknahme()
   CASE 4
     CALL OFFICE()
   CASE 5
     CALL SETUP()
   CASE 6
     CALL Tarife()
   END SELECT 
 WEND 
 _formend% = 0
 END SUB 
 
 
 
 ' ======================================================================
 ' Routinen fÅr Formular "MEDIA"
 ' ======================================================================
 
SUB FILME()
 _formend% = 0
 SET STATUS ""
 WHILE _formend% = 0
   IF FORM <> "FILME" THEN 
     OPEN FORM "FILME" PAGE 1
     CALL MNFILME()
     CALL FILMEIntro()
   END IF 
   MENU ON 
   MOUSE ON 
   Medium$ = Medium$(Medium.filme)
   FILMEsub% = 0
   CALL FLMUpdate()
   FORM 
   WAIT MOUSE OR KEY OR MENU 
   SELECT CASE FILMEsub%
   CASE 99
     ENTER nummer.FILME,0
   END SELECT 
 WEND 
 cuvi% = 0
 _formend% = 0
 END SUB 
 
